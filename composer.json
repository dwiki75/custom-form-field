{
  "name": "dwiki/custom-form-field",
  "description": "Egy egyszerű, adminból konfigurálható Tailwind input mező Craft CMS-hez",
  "type": "craft-plugin",
  "require": {
    "craftcms/cms": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "dwiki\\customformfield\\": "src/"
    }
  },
  "extra": {
    "handle": "custom-form-field",
    "class": "dwiki\\customformfield\\CustomFormField"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:dwiki75/custom-form-field.git"
    }
  ]
}